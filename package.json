{
  "name": "amplify-japan-user-group",
  "version": "1.0.0",
  "private": true,
  "description": "Amplify 日本ユーザーグループのウェブサイト",
  "author": "Amplify Japan User Group",
  "keywords": [
    "gatsby"
  ],
  "packageManager": "npm@9.6.6",
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "scripts": {
    "prestart": "npm run fetch:contents",
    "start": "gatsby develop",
    "prebuild": "npm run fetch:contents",
    "build": "gatsby clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "npm run lint",
    "lint": "prettier src scripts gatsby-config.js",
    "lint:fix": "prettier --write src scripts gatsby-config.js gatsby-node.js",
    "fetch:contents": "node scripts/fetchContents.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@gatsbyjs/reach-router": "^2.0.1",
    "@mui/icons-material": "latest",
    "@mui/lab": "latest",
    "@mui/material": "latest",
    "classnames": "latest",
    "gatsby": "next",
    "gatsby-plugin-gtag": "latest",
    "gatsby-plugin-manifest": "next",
    "gatsby-plugin-sitemap": "next",
    "gatsby-remark-component": "latest",
    "gatsby-source-filesystem": "next",
    "gatsby-transformer-remark": "next",
    "prettier": "latest",
    "react": "latest",
    "react-dom": "latest",
    "rehype-react": "latest",
    "remark-parse": "latest",
    "remark-rehype": "latest",
    "unified": "latest"
  },
  "devDependencies": {
    "glob": "latest",
    "node-fetch": "latest",
    "uuid": "latest"
  }
}
